{
  "name": "somanyconflicts",
  "displayName": "So Many Conflicts",
  "description": "No more headache on multiple conflicts after merging!",
  "version": "0.0.1",
  "publisher": "Symbolk",
  "repository": {
    "type": "git",
    "url": "https://github.com/Symbolk/somanyconflicts"
  },
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:somanyconflicts.somany",
    "onCommand:somanyconflicts.next",
    "onCommand:somanyconflicts.resolve"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "somanyconflicts.somany",
        "title": "So Many Conflicts: Where to start?"
      },
      {
        "command": "somanyconflicts.next",
        "title": "So Many Conflicts: What's next?"
      },
      {
        "command": "somanyconflicts.resolve",
        "title": "So Many Conflicts: How to resolve?"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "simple-git": "^2.38.0"
  }
}
